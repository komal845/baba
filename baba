style.css
body{
  font-family: 'Courier New', Courier, monospace;
  margin: 80px;
  background:white;
}

.container{
  width: 8000px;
  background: white;
  border: 10px;
  margin:0 auto;
  padding: 16px;
  border-radius: 6px;
  box-shadow: 0 0 6px rgba(0,0,0,0.1);

}
.td,tr{
  width: 800px;
  background: wheat;
  border: 10px;
  margin:10px solid black;
  padding: 16px;
  border-radius: 6px solid black;
  box-shadow: 0 0 6px rgba(0,0,0,0.1);

}
table{
  width: 800px;
  background: rgb(18, 18, 11);
  border: 10px;
  margin:10px solid black;
  padding: 16px;
  border-radius: 6px solid black;
  box-shadow: 0 0 6px rgba(0,0,0,0.1);


}

