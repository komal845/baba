<body>

<div class="form-container">
  <h2>Create Account</h2>
  <form id="registrationForm" novalidate>
    <div class="form-group">
      <label>Full Name</label>
      <input type="text" id="name"  />
      <div class="error" id="nameError"></div>
    </div>

    <div class="form-group">
      <label>Email</label>
      <input type="email" id="email" />
      <div class="error" id="emailError"></div>
    </div>

    <div class="form-group">
      <label>Password</label>
      <input type="password" id="password" />
      <div class="error" id="passwordError"></div>
    </div>

    <div class="form-group">
      <label>Gender</label>
      <div class="gender-group">
        <label><input type="radio" name="gender" value="Male"  /> Male</label>
        <label><input type="radio" name="gender" value="Female" /> Female</label>
      </div>
      <div class="error" id="genderError"></div>
    </div>

    <div class="form-group">
      <label>Date of Birth</label>
      <input type="date" id="dob" />
      <div class="error" id="dobError"></div>
    </div>

    <button type="submit">Register</button>
  </form>
</div>

<script>
  const form = document.getElementById("registrationForm");

  form.addEventListener("submit", function (e) {
    e.preventDefault();
    const isValid =
      validateName() &&
      validateEmail() &&
      validatePassword() &&
      validateGender() &&
      validateDOB();

    if (isValid) {
      alert("Registration Successful!");
      form.reset();
      document.querySelectorAll(".error").forEach(el => el.innerText = "");
    }
  });

  function validateName() {
    const name = document.getElementById("name").value.trim();
    const nameError = document.getElementById("nameError");
    if (name === "") {
      nameError.innerText = "Name is required";
      return false;
    }
    nameError.innerText = "";
    return true;
  }

  function validateEmail() {
    const email = document.getElementById("email").value.trim();
    const emailError = document.getElementById("emailError");
    const pattern = /^[^ ]+@[^ ]+\.[a-z]{2,3}$/;
    if (email === "") {
      emailError.innerText = "Email is required";
      return false;
    } else if (!pattern.test(email)) {
      emailError.innerText = "Enter a valid email";
      return false;
    }
    emailError.innerText = "";
    return true;
  }

  function validatePassword() {
    const password = document.getElementById("password").value;
    const passwordError = document.getElementById("passwordError");
    if (password.length < 6) {
      passwordError.innerText = "Password must be at least 6 characters";
      return false;
    }
    passwordError.innerText = "";
    return true;
  }

  function validateGender() {
    const gender = document.querySelector("input[name='gender']:checked");
    const genderError = document.getElementById("genderError");
    if (!gender) {
      genderError.innerText = "Please select gender";
      return false;
    }
    genderError.innerText = "";
    return true;
  }

  function validateDOB() {
    const dob = document.getElementById("dob").value;
    const dobError = document.getElementById("dobError");
    if (!dob) {
      dobError.innerText = "Date of Birth is required";
      return false;
    }
    dobError.innerText = "";
    return true;
  }
</script>

</body>
</html>

jsfunctions event handler
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Event Handlers in JavaScript</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f0f2f5;
            text-align: center;
            padding: 50px;
        }

        h2 {
            color: #333;
            margin-bottom: 30px;
        }

        button {
            padding: 10px 20px;
            background-color: #007bff;
            color: white;
            border: none;
            border-radius: 6px;
            cursor: pointer;
            font-size: 16px;
        }

        button:hover {
            background-color: #0056b3;
        }

        input[type="text"] {
            padding: 10px;
            width: 250px;
            border-radius: 5px;
            border: 1px solid #ccc;
            margin: 10px;
        }

        form input[type="submit"] {
            padding: 10px 20px;
            background-color: #28a745;
            color: white;
            border: none;
            border-radius: 6px;
            cursor: pointer;
            font-size: 16px;
        }

        form input[type="submit"]:hover {
            background-color: #218838;
        }

        div {
            transition: background-color 0.3s ease;
            margin: 0 auto;
            width: 250px;
            border-radius: 5px;
            font-size: 16px;
        }
    </style>
</head>
<body>

    <h2>JavaScript Event Handler Demo</h2>

    <!-- Button Click Event -->
    <button onclick="sayHello()">Click Me</button><br><br>

    <!-- Mouse Over Event -->
    <div onmouseover="changeColor(this)" onmouseout="resetColor(this)" style="width: 200px; padding: 20px; background-color: lightgray;">
        Hover over me
    </div><br>

    <!-- Input Focus Event -->
    <input type="text" id="nameField" placeholder="Focus me" onfocus="highlight(this)" onblur="removeHighlight(this)"><br><br>

    <!-- Form Submit Event -->
    <form onsubmit="return submitForm()">
        <input type="text" placeholder="Type something" id="inputField">
        <input type="submit" value="Submit">
    </form>

    <script>
        // Button click event
        function sayHello() {
            alert("Hello! You clicked the button.");
        }

        // Mouse over and out events
        function changeColor(element) {
            element.style.backgroundColor = "orange";
        }

        function resetColor(element) {
            element.style.backgroundColor = "lightgray";
        }

        // Focus and blur events
        function highlight(field) {
            field.style.backgroundColor = "#e0ffff";
        }

        function removeHighlight(field) {
            field.style.backgroundColor = "white";
        }

        // Form submission event
        function submitForm() {
            let input = document.getElementById("inputField").value;
            alert("You submitted: " + input);
            return false; // Prevent actual form submission
        }
    </script>

</body>
</html>

