<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Fibonacci Series Generator</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f0f2f5;
            padding: 40px;
            text-align: center;
        }

        .container {
            background: white;
            max-width: 400px;
            margin: auto;
            padding: 30px;
            border-radius: 10px;
            box-shadow: 0 0 10px rgba(0,0,0,0.1);
        }

        input, button {
            padding: 10px;
            margin: 10px;
            font-size: 16px;
            border-radius: 6px;
            border: 1px solid #ccc;
        }

        button {
            background-color: #007bff;
            color: white;
            border: none;
        }

        button:hover {
            background-color: #0056b3;
        }

        #result {
            margin-top: 15px;
            color: #333;
        }
    </style>
</head>
<body>
    <div class="container">
        <h2>Fibonacci Series Generator</h2>
        <label for="count">Enter number of terms:</label>
        <input type="number" id="count" min="1" placeholder="e.g. 10">
        <button onclick="generateFibonacci()">Generate</button>

        <h3>Result:</h3>
        <p id="result"></p>
    </div>

    <script>
        function generateFibonacci() {
            const n = parseInt(document.getElementById("count").value);
            const result = document.getElementById("result");

            if (isNaN(n) || n <= 0) {
                result.innerHTML = "Please enter a valid positive number.";
                return;
            }

            let fib = [0, 1];
            for (let i = 2; i < n; i++) {
                fib[i] = fib[i - 1] + fib[i - 2];
            }

            result.innerHTML = fib.slice(0, n).join(', ');
        }
    </script>
</body>
</html
